<?xml version='1.0'?>

<robot name="slam_project" xmlns:xacro="http://www.ros.org/wiki/xacro">
<xacro:include filename="$(find slam_project)/urdf/slam_project.gazebo" />
  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0.0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <link name='chassis'>
<pose> 0 0 .2 0 0 0 </pose>
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.004" ixy="0" ixz="0"
          iyy="0.004" iyz="0"
          izz="0.004"
      />
    </inertial>

    <collision name='chassis_collision'>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <sphere radius=".1"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius=".1"/>
      </geometry>
    </visual>
</link>

<link name='back_left_caster'>
<inertial>
<mass value=".05"/>
<inertia ixx=".0001" iyy=".00014" izz=".00005"
         ixy="0" ixz="0" iyz="0" />
</inertial>

<collision name='back_left_caster_collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".1 .05 .15"/>
      </geometry>
      <surface>
      <friction>
      <ode>
      <mu>0</mu>
     <mu2>0</mu2>
     <slip1>1.0</slip1>
     <slip2>1.0</slip2>
     </ode>
    </friction>
    </surface>
    </collision>

    <visual name='back_left_caster_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.1 .05 .15"/>
      </geometry>
    </visual>
</link>

<joint type="fixed" name="back_left_caster_joint">
<origin xyz="-.075 .087 -.075" rpy="0 0 0"/>
<parent link="chassis"/>
<child link="back_left_caster"/>
</joint>

<link name="back_right_caster">
<inertial>
<mass value=".05"/>
<inertia ixx=".0001" iyy=".00014" izz=".00005"
         ixy="0" ixz="0" iyz="0" />
</inertial>

<collision name='back_right_caster_collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".1 .05 .15"/>
      </geometry>
      <surface>
      <friction>
      <ode>
      <mu>0</mu>
      <mu2>0</mu2>
      <slip1>1.0</slip1>
      <slip2>1.0</slip2>
      </ode>
      </friction>
     </surface>
    </collision>

    <visual name='back_right_caster_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.1 .05 .15"/>
      </geometry>
    </visual>
</link>

<joint type="fixed" name="back_right_caster_joint">
<origin xyz="-.075 -.087 -.075" rpy="0 0 0"/>
<parent link="chassis"/>
<child link="back_right_caster"/>
</joint>

<link name="front_caster">
<inertial>
<mass value=".05"/>
<inertia ixx=".0001" iyy=".00014" izz=".00005"
         ixy="0" ixz="0" iyz="0" />
</inertial>

    <collision name='front_caster_collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.1 .05 .15"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>

    <visual name='front_caster_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.1 .05 .15"/>
      </geometry>
    </visual>

</link>

<joint type="continuous" name="front_caster_joint">
<origin xyz=".075 0 -.075" rpy="0 0 0"/>
<parent link="chassis"/>
<child link="front_caster"/>
<axis xyz="0 0 1"/>
</joint>

<link name="back_left_wheel">
<inertial>
<mass value="2.0"/>
<origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
<inertia ixx="0.0017" ixy="0" ixz="0"
         iyy="0.0017" iyz="0" izz=".0004"/>
</inertial>

<collision name="back_left_wheel_collision">
<origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
<geometry>
<cylinder radius="0.05" length="0.05"/>
</geometry>
</collision>

<visual name="back_left_wheel_visual">
<origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
<geometry>
<cylinder radius="0.05" length="0.05"/>
</geometry>
</visual>
</link>

<joint type="continuous" name="left_wheel_hinge">
<origin xyz="0 0 -.075" rpy="0 0 0"/>
<child link="back_left_wheel"/>
<parent link="back_left_caster"/>
<axis xyz="0 1 0" rpy="0 0 0"/>
</joint>

<link name="back_right_wheel">
<inertial>
<mass value="2.0"/>
<origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
<inertia ixx="0.0017" ixy="0" ixz="0"
         iyy="0.0017" iyz="0" izz=".0004"/>
</inertial>

<collision name="back_right_wheel_collision">
<origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
<geometry>
<cylinder radius="0.05" length="0.05"/>
</geometry>
</collision>

<visual name="back_right_wheel_visual">
<origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
<geometry>
<cylinder radius="0.05" length="0.05"/>
</geometry>
</visual>
</link>

<joint type="continuous" name="right_wheel_hinge">
<origin xyz="0 0 -.075" rpy="0 0 0"/>
<child link="back_right_wheel"/>
<parent link="back_right_caster"/>
<axis xyz="0 1 0" rpy="0 0 0"/>
</joint>

<link name="front_wheel">
<inertial>
<mass value="2.0"/>
<origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
<inertia ixx="0.0017" ixy="0" ixz="0"
         iyy="0.0017" iyz="0" izz="0.0004"/>
</inertial>

<collision name="front_wheel_collision">
<origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
<geometry>
<cylinder radius="0.05" length="0.05"/>
</geometry>
</collision>

<visual name="front_wheel_visual">
<origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
<geometry>
<cylinder radius="0.05" length="0.05"/>
</geometry>
</visual>
</link>

<joint type="continuous" name="front_wheel_hinge">
<origin xyz="0 0 -.075" rpy="0 0 0"/>
<child link="front_wheel"/>
<parent link="front_caster"/>
<axis xyz="0 0 1" rpy="0 0 0"/>
<limit effort="10000" velocity="1000"/>
<joint_properties damping="1.0" friction="1.0"/>
</joint>

<link name="belt">
<inertial>
<mass value="0.5"/>
<origin xyz="0 0 0" rpy="0 0 0"/>
<inertia ixx=".005" ixz="0" iyy=".005"
         ixy="0" iyz="0" izz=".0017"/>
</inertial>

<collision name="belt_collision">
<origin xyz="0 0 0" rpy="0 0 0"/>
<geometry>
<cylinder radius =".2" length=".01"/>
</geometry>
</collision>

<visual name="belt_visual">
<origin xyz="0 0 0" rpy="0 0 0"/>
<geometry>
<cylinder radius="0.2" length=".01"/>
</geometry>
</visual>
</link>

<joint type="continuous" name="belt_joint">
<origin xyz="0 0 0" rpy="0 0 0"/>
<child link="belt"/>
<parent link="chassis"/>
<axis xyz="0 0 1"/>
</joint>

<link name="hokuyo">
<inertial>
<mass value="0.1"/>
<origin xyz="0 0 0" rpy="0 0 0"/>
<inertia ixx="1e-6" ixy="0" ixz="0"
         iyy="1e-6" iyz="0" izz="1e-6"/>
</inertial>
<collision name="hokuyo_collision">
<origin xyz="0 0 0" rpy="0 0 0"/>
<geometry>
<box size="0.1 0.1 0.1"/>
</geometry>
</collision>
<visual name="hokuyo_visual">
<origin xyz="0 0 0" rpy="0 0 0"/>
<geometry>
<mesh filename="package://slam_project/meshes/hokuyo.dae"/>
</geometry>
</visual>
</link>

<joint type="fixed" name="hokuyo_joint">
<origin xyz="0 0 0.1" rpy="0 0 0"/>
<child link="hokuyo"/>
<parent link="chassis"/>
</joint>

 <link name='camera_link'>

    <inertial>
      <mass value="0.564"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.003881243" ixy="0" ixz="0"
          iyy="0.000498940" iyz="0"
          izz="0.003879257"
      />
    </inertial>

    <collision name='camera_collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.07271 0.27794 0.073"/>
      </geometry>
    </collision>

    <visual name='camera_visual'>
      <origin xyz="0 0 0" rpy="0 0 1.571"/>
      <geometry>
        <mesh filename="package://slam_project/meshes/kinect.dae"/>
      </geometry>
     <material name="red"/>
    </visual>
</link>

<link name="camera_rgb_frame"/>
<link name="camera_rgb_optical_frame"/>
<link name="camera_depth_frame"/>
<link name="camera_depth_optical_frame"/>

<joint type="fixed" name="camera_rgb_joint">
    <origin xyz="0.12 0  0.0415" rpy="0 0 0"/>
    <child link="camera_rgb_frame"/>
    <parent link="chassis"/>
  </joint>

<joint name="camera_rgb_optical_joint" type="fixed">
<origin xyz="0 0 0" rpy="-1.571 0 -1.571"/>
<parent link="camera_rgb_frame"/>
<child link="camera_rgb_optical_frame"/>
</joint>

    <joint name="camera_joint" type="fixed">
      <origin xyz="-0.031 0 -0.016" rpy="0 0 0"/>
      <parent link="camera_rgb_frame"/>
      <child link="camera_link"/>
    </joint>

<joint name="camera_depth_joint" type="fixed">
<origin xyz="0 0 0" rpy="0 0 0"/>
<parent link="camera_rgb_frame"/>
<child link="camera_depth_frame"/>
</joint>

<joint name="camera_depth_optical_joint" type="fixed">
<origin xyz="0 0 0" rpy="-1.571 0 -1.571"/>
<parent link="camera_depth_frame"/>
<child link="camera_depth_optical_frame"/>
</joint>

</robot>
